<resources>
    <declare-styleable name="mv_MyPie">
        <attr name="mv_min"
            format="float"/>
        <attr name="mv_max"
            format="float"/>
        <attr name="mv_from"
            format="float"/>
        <attr name="mv_to"
            format="float"/>
        <attr name="mv_pieColor"
            format="color"/>
        <attr name="mv_ovalColor"
            format="color"/>
    </declare-styleable>
    <declare-styleable name="mv_MyLLDecorator">
        <attr name="mv_decoration"
            format="reference"/>
    </declare-styleable>
    <declare-styleable name="mv_MyScrollBehavior">
        <attr name="mv_behavior_property" format="string"/>
        <attr name="mv_behavior_scroll_min" format="integer"/>
        <attr name="mv_behavior_scroll_max" format="integer"/>
        <attr name="mv_behavior_min" format="integer"/>
        <attr name="mv_behavior_max" format="integer"/>
    </declare-styleable>
</resources>
